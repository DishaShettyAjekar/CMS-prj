@model IEnumerable<MVCProject.tbl_skills>

@{
    ViewBag.Title = "Index";
}

<h2>User List using Jquery Datatable</h2>
<table id="UserList" class="display table table-striped table-bordered mt-4" style="width:100%">
    <thead>
        <tr>
            <th>User Id</th>
            <th>User Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Skills</th>
            <th>Photo</th>
            <th>Role</th>
            <th>Action</th>
        </tr>
    </thead>
</table>
<div class="modal" id="myModal">
    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Modal Heading</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                Modal body..
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>

        </div>
    </div>
</div>
